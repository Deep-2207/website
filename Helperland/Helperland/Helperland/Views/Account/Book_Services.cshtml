@model BookService
@{
    ViewData["Title"] = "BookAService";
}
<section class="first-img">
    <img src="~/img/Book-Service/book-service-banner.jpg" alt="">
</section>


<section class="Book-service pt-0" id="FAQ">
    <div class="max-width-1170 pt-0">
        <div class="row justify-content-center text-center">
            <div class="col-12 mb-90" style="position: relative;">
                <h2 class="headding-star">Set Up your cleaning services</h2>
            </div>
        </div>

        <div class="book-service-body row">

            <div class="container container-max-width mb-60 col-md-8">
                <ul class="nav nav-pills mb-30 col-md-12" id="pills-tab" role="tablist">
                    <li class="nav-item col b-l-1 b-b-1" role="presentation">
                        <button class="nav-link" id="pills-home-tab" data-bs-toggle="pill" data-bs-target="#pills-setup"
                                type="button" role="tab" aria-controls="pills-home" aria-selected="true">
                            <img src="~/img/Book-Service/setup-service.png" alt="" class="setup-img"> <span>Setup Service</span>
                        </button>
                    </li>
                    <li class="nav-item col b-l-1 b-b-1" role="presentation">
                        <button class="nav-link " id="pills-profile-tab" data-bs-toggle="pill" data-bs-target="#pills-paln"
                                type="button" role="tab" aria-controls="pills-profile" aria-selected="false">
                            <img src="~/img/Book-Service/schedule.png" alt="" class="schedule-img"> <span> Schedule & Plan</span>
                        </button>
                    </li>
                    <li class="nav-item col b-l-1 b-b-1" role="presentation">
                        <button class="nav-link" id="pills-profile-tab" data-bs-toggle="pill"
                                data-bs-target="#pills-details" type="button" role="tab" aria-controls="pills-profile"
                                aria-selected="false">
                            <img src="~/img/Book-Service/details.png" alt="" class="details-img"> <span>
                                Your Details
                            </span>
                        </button>
                    </li>
                    <li class="nav-item col b-l-1 b-b-1" role="presentation">
                        <button class="nav-link  active" id="pills-profile-tab" data-bs-toggle="pill" data-bs-target="#pills-payment"
                                type="button" role="tab" aria-controls="pills-profile" aria-selected="false">
                            <img src="~/img/Book-Service/payment.png" alt="" class="payment-img"> <span>
                                Make Payment
                            </span>
                        </button>
                    </li>
                </ul>
                <div class="tab-content" id="pills-tabContent">
                    <div class="tab-pane fade" id="pills-setup" role="tabpanel" aria-labelledby="pills-home-tab">
                        <form asp-action="Book_Services" method="post" asp-controller="account">
                            <div class="row">
                                <div class="col-md-12">
                                    <h6 class="fw-bold">Enter your Postal Code</h6>
                                    <input type="text" name="" id="" asp-for="CheckPostalCode" class="form-control mb-3 me-3">
                                    <button class="btn btncontinue" type="submit">Check Availability</button>
                                </div>
                            </div>
                        </form>
                    </div>
                    <div class="tab-pane fade " id="pills-paln" role="tabpanel" aria-labelledby="pills-profile-tab">
                        <div class="plan-body">
                            <div class="quantity">
                                <h2>Select number of rooms and bath</h2>
                                <div class="quantuty-type row">
                                    <div class="form-group">

                                        <select class="form-select" id="exampleFormControlSelect1">
                                            <option>1</option>
                                            <option>2</option>
                                            <option>3</option>
                                            <option>4</option>
                                            <option>5</option>
                                        </select>
                                    </div>
                                    <div class="form-group">

                                        <select class="form-select" id="exampleFormControlSelect1">
                                            <option>1</option>
                                            <option>2</option>
                                            <option>3</option>
                                            <option>4</option>
                                            <option>5</option>
                                        </select>
                                    </div>


                                </div>

                            </div>

                            <hr class="m-tb-30">

                            <div class="timedate">
                                <div class="card-body">
                                    <div class="headding">
                                        When do you need the cleaner?
                                    </div>
                                    <div class="datetime">
                                        <div class="datetime">
                                            <!-- <input type="text" placeholder="From Date" class="form-control input-element width-135" onfocus="(this.type='date')" onblur="(this.type='text')"> -->
                                            <input type="date" id="txtFromDate" class="calender-icon form-control input-element width-135">
                                            <!-- <input type="date" class="form-control input-element width-135" id=""> -->
                                        </div>
                                        <div class="form-group">

                                            <select class="form-select" id="exampleFormControlSelect1">
                                                <option>1</option>
                                                <option>2</option>
                                                <option>3</option>
                                                <option>4</option>
                                                <option>5</option>
                                            </select>
                                        </div>
                                    </div>

                                </div>
                                <div class="card-body">
                                    <div class="headding">
                                        How long do you need your cleaner to stay?
                                    </div>

                                    <div class="time">

                                        <div class="form-group">

                                            <select class="form-select" id="exampleFormControlSelect1">
                                                <option>1</option>
                                                <option>2</option>
                                                <option>3</option>
                                                <option>4</option>
                                                <option>5</option>
                                            </select>
                                        </div>
                                    </div>

                                </div>
                            </div>

                            <hr class="m-tb-30">
                            <div class="extra-services">
                                <h2>Extra-services</h2>
                                <div class="extra-services-icone p-tb-15">
                                    <div class="custom-control custom-checkbox">
                                        <input type="checkbox" class="custom-control-input" id="cabinets">
                                        <label class="custom-control-label" for="cabinets">
                                            <div class="icone-box">
                                                <!-- <div class="img-container"> -->
                                                <img src="~/img/prices/3.png" alt="forma-1" class="img-size">
                                                <!-- </div> -->
                                            </div>
                                            <span class="extra-service-name">Inside cabinets</span>
                                        </label>
                                    </div>
                                    <div class="custom-control custom-checkbox">
                                        <input type="checkbox" class="custom-control-input" id="fridge">
                                        <label class="custom-control-label" for="fridge">
                                            <div class="icone-box">

                                                <img src="~/img/prices/5.png" alt="group-22" class="img-size">

                                            </div>
                                            <span class="extra-service-name">Inside fridge</span>
                                        </label>
                                    </div>
                                    <div class="custom-control custom-checkbox">
                                        <input type="checkbox" class="custom-control-input" id="oven">
                                        <label class="custom-control-label" for="oven">
                                            <div class="icone-box">

                                                <img src="~/img/prices/4.png" alt="group-22" class="img-size">

                                            </div>
                                            <span class="extra-service-name">Inside oven</span>
                                        </label>
                                    </div>
                                    <div class="custom-control custom-checkbox">
                                        <input type="checkbox" class="custom-control-input" id="Laundry">
                                        <label class="custom-control-label" for="Laundry">
                                            <div class="icone-box">

                                                <img src="~/img/prices/2.png" alt="group-22" class="img-size">

                                            </div>
                                            <span class="extra-service-name">Laundry wash & dry</span>
                                        </label>
                                    </div>
                                    <div class="custom-control custom-checkbox">
                                        <input type="checkbox" class="custom-control-input" id="windows">
                                        <label class="custom-control-label" for="windows">
                                            <div class="icone-box">

                                                <img src="~/img/prices/1.png" alt="group-22" class="img-size">

                                            </div>
                                            <span class="extra-service-name">Interior windows</span>
                                        </label>
                                    </div>

                                </div>
                            </div>

                            <hr class="m-tb-30">

                            <div class="comments mb-30">

                                <div class="form-group">
                                    <label for="comment" class="mb-10">Comment</label>
                                    <textarea class="form-control" id="comment" rows="3" placeholder="Comments"
                                              maxlength="500"></textarea>
                                </div>
                            </div>

                            <div class="pets">
                                <div class="form-check">
                                    <input class="form-check-input" type="checkbox" value="" id="defaultCheck1">
                                    <label class="form-check-label" for="defaultCheck1">
                                        I have pets at home
                                    </label>
                                </div>
                            </div>

                            <hr class="m-tb-30">

                            <div class="continue text-right text-small-center">
                                <button class="btn btn-primary btn-lg btncontinue">Continue</button>
                            </div>

                        </div>
                    </div>
                    <!-- </div> -->
                    <div class="tab-pane fade" id="pills-details" role="tabpanel" aria-labelledby="pills-home-tab">
                        <div class="Address-list">
                            <h2 class="headding">
                                Enter your contact details, so we can serve you in better way!
                            </h2>
                            <div class="Address-list-item">
                                <div class="form-check">
                                    <input class="form-check-input" type="radio" name="exampleRadios" id="exampleRadios1"
                                           value="option1" checked>
                                    <label class="form-check-label" for="exampleRadios1">
                                        <p class="d-block m-0">
                                            <span class="point fw-bold">Address: </span> wdwd 44, Tambach-Dietharz
                                            99897
                                        </p>

                                        <p class="d-block m-0"> <span class="point fw-bold">Phone number: </span> 9988556644</p>
                                    </label>
                                </div>
                                <div class="form-check">
                                    <input class="form-check-input" type="radio" name="exampleRadios" id="exampleRadios1"
                                           value="option1" checked>
                                    <label class="form-check-label" for="exampleRadios1">
                                        <p class="d-block m-0">
                                            <span class="point fw-bold">Address: </span> wdwd 44, Tambach-Dietharz
                                            99897
                                        </p>

                                        <p class="d-block m-0"> <span class="point fw-bold">Phone number: </span> 9988556644</p>
                                    </label>
                                </div>
                                <div class="form-check">
                                    <input class="form-check-input" type="radio" name="exampleRadios" id="exampleRadios1"
                                           value="option1" checked>
                                    <label class="form-check-label" for="exampleRadios1">
                                        <p class="d-block m-0">
                                            <span class="point fw-bold">Address: </span> wdwd 44, Tambach-Dietharz
                                            99897
                                        </p>

                                        <p class="d-block m-0"> <span class="point fw-bold">Phone number: </span> 9988556644</p>
                                    </label>
                                </div>
                            </div>

                            <div class="">
                                <button class="btncontinue d-inline-block btnaddress" id="btnNewAddress">+ Add New Address</button>

                            </div>

                            <div class="newAddress">
                                <div class="row row-cols-1 row-cols-md-2 g-4 mt-0">
                                    <div class="col">
                                        <label for="exampleInputEmail1" class="form-label">Street name</label>
                                        <input type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp"
                                               placeholder="Street name">
                                    </div>
                                    <div class="col">
                                        <label for="exampleInputEmail1" class="form-label">House number</label>
                                        <input type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp"
                                               placeholder="House number">
                                    </div>
                                </div>
                                <div class="row row-cols-1 row-cols-md-2 g-4 mt-0">
                                    <div class="col">
                                        <label for="exampleInputEmail1" class="form-label">Postal code</label>
                                        <input type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp"
                                               placeholder="Postal code">
                                    </div>
                                    <div class="col">
                                        <label for="exampleInputEmail1" class="form-label">City</label>
                                        <select class="form-select" id="exampleInputEmail1" aria-describedby="emailHelp"
                                                placeholder="City">
                                            <option>--Select City--</option>
                                            <option>--City 1--</option>
                                            <option>--City 2--</option>
                                        </select>
                                    </div>
                                </div>

                                <div class="row row-cols-1 row-cols-md-2 g-4 mt-0">
                                    <div class="col">
                                        <label for="exampleInputEmail1" class="form-label">Phone Number</label>
                                        <div class="input-group">
                                            <div class="input-group-prepend">
                                                <div class="input-group-text">+49</div>
                                            </div>
                                            <input type="text" class="form-control" id="inlineFormInputGroupmobilenumber" placeholder="Mobile Number">
                                        </div>
                                    </div>
                                </div>

                                <div class="btnaction mt-2">
                                    <div class="save">
                                        @*<input class="btn-rounded" / >*@
                                        <button class="btn-rounded btnsave">Save</button>
                                    </div>
                                    <div class="Cancel ms-2">
                                        @*<input class="btn-rounded" />*@
                                        <button class="btn-rounded btnCancel">Cancel</button>
                                    </div>
                                </div>
                            </div>
                            <div class="continue text-right text-small-center">
                                <button class="btn btn-primary btn-lg btncontinue">Continue</button>
                            </div>

                        </div>
                    </div>
                    <div class="tab-pane fade show active" id="pills-payment" role="tabpanel" aria-labelledby="pills-home-tab">
                        <div class="payment">
                            <h2 class="headding">
                                Pay securely with Helperland payment gateway!
                            </h2>
                            <div class="row row-cols-1 row-cols-md-2 g-4 mt-0">
                                <div class="col mt-0">
                                    <label for="exampleInputEmail1" class="form-label">Promo code (optional)</label>
                                    <input type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp"
                                           placeholder="Promo code (optional)">
                                </div>
                                <div class="save">
                                    @*<input class="btn-rounded" / >*@
                                    <button class="btn-rounded btnsave mt-2">Apply</button>
                                </div>
                            </div>
                            <hr class="m-tb-30">
                            <div class="mb-3 position-relative payment-card">
                                <input type="text" class="form-control card-number mw-100" placeholder="Card Number">
                                <img src="~/img/Book-Service/credit-card-inputTagImg.png" />
                                <input type="text" class="form-control month-year" placeholder="MM/YY" aria-label="Username">
                                <input type="text" class="form-control CC" placeholder="CVC" aria-label="Username">
                            </div>

                            <div class="accept-card float-right">
                                <p class="float-right ms-auto">Accepted cards:</p>
                                <img src="~/img/Book-Service/cardImg.png" />
                            </div>
                            <br />
                            <hr class="m-tb-30">

                            <div class="payment-check form-check">

                                <input class="form-check-input" type="checkbox" value="" id="flexCheckDefault">
                                <label class="form-check-label" for="flexCheckDefault">
                                    I accepted terms and conditions, the cancellation policy and the privacy policy. I confirm that Helperland starts to execute the contract before the expiry of the withdrawal period and I lose my right of withdrawal as a consumer with full performance of the contract.
                                </label>

                            </div>
                            <hr class="m-tb-30">
                            <div class="continue text-right text-small-center">
                                <button class="btn btn-primary btn-lg btncontinue">Completed Booking</button>
                            </div>

                        </div>
                    </div>
                </div>



            </div>
            <div class="col-md-4 d-none d-md-block">
                <div class="card mb-30">
                    <h2 class="card-header">Payment summary</h2>
                    <div class="card-body">

                        <p class="timedate">11/01/2022 08:00</p>
                        <p class="bed-bath">1 bed, 1 bath</p>

                    </div>
                    <div class="card-body">
                        <p class="c-6464">Duration</p>
                        <p class="tmp-time">Basic <span class="text-right">3hrs</span></p>
                        <p class="tmp-time extra">inside cabinets(extra) <span class="text-right">30min</span></p>
                        <hr>
                        <p class="total-time">Total Service Time <span>3.5hrs</span></p>
                    </div>
                    <div class="card-body">
                        <p class="singal-charg">Per cleaning <span>$87</span></p>
                        <p class="singal-charg">Discount <span>-$27</span></p>
                    </div>
                    <div class="card-body total-payment">
                        <p class="total-charg">Total payment <span>$63</span> </p>

                        <p class="effective-price">Effective Price <span>$50.4</span> </p>
                        <p class="bounce"><span class="text-danger">*</span> you will save 20% according to §35a EStG.</p>
                    </div>
                    <h2 class="card-footer">
                        <img src="~/img/Book-Service/smiley.png" alt="">See what is always included
                    </h2>
                </div>
                <div class="FAQ">
                    <div class="headding text-center">
                        Questions..?
                    </div>
                    <div class="accordion accordion-flush mb-3" id="accordionExample">
                        <div class="accordion-item">
                            <h3 class="accordion-header" id="headingOne">

                                <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse"
                                        data-bs-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                                    Which Helperland professional will come to my place?
                                </button>
                            </h3>
                            <div id="collapseOne" class="accordion-collapse collapse" aria-labelledby="headingOne"
                                 data-bs-parent="#accordionExample">
                                <div class="accordion-body">
                                    ans no- 1
                                </div>
                            </div>
                        </div>
                        <div class="accordion-item">
                            <h3 class="accordion-header" id="headingTwo">
                                <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse"
                                        data-bs-target="#collapseTwo" aria-expanded="false" aria-controls="collapseTwo">
                                    Which Helperland professional will come to my place?
                                </button>
                            </h3>
                            <div id="collapseTwo" class="accordion-collapse collapse" aria-labelledby="headingTwo"
                                 data-bs-parent="#accordionExample">
                                <div class="accordion-body">
                                    ans no- 2
                                </div>
                            </div>
                        </div>
                        <div class="accordion-item">
                            <h3 class="accordion-header" id="headingThree">
                                <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse"
                                        data-bs-target="#collapseThree" aria-expanded="false" aria-controls="collapseThree">
                                    Which Helperland professional will come to my place?
                                </button>
                            </h3>
                            <div id="collapseThree" class="accordion-collapse collapse" aria-labelledby="headingThree"
                                 data-bs-parent="#accordionExample">
                                <div class="accordion-body">
                                    ans no- 3
                                </div>
                            </div>
                        </div>
                        <div class="accordion-item">
                            <h3 class="accordion-header" id="headingfour">
                                <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse"
                                        data-bs-target="#collapsefour" aria-expanded="false" aria-controls="collapsefour">
                                    Which Helperland professional will come to my place?
                                </button>
                            </h3>
                            <div id="collapsefour" class="accordion-collapse collapse" aria-labelledby="headingfour"
                                 data-bs-parent="#accordionExample">
                                <div class="accordion-body">
                                    ans no- 4
                                </div>
                            </div>
                        </div>

                    </div>
                    <div class="for-more">
                        <a href="#" class="formorehelp">For More Help</a>
                    </div>
                </div>

            </div>

        </div>

    </div>

</section>

