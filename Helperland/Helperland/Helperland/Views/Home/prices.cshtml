@{
    ViewData["Title"] = "Prices";
}
<section class="first-img">
    <img src="~/img/prices/group-16_2.png" alt="">
</section>

<section class="prices pt-0" id="prices">
    <div class="max-width-1140  border-b-1 pt-0">
        <div class="row justify-content-center text-center">
            <div class="col-12 mb-5 mb-75" style="position: relative;">
                <h2 class="headding-star">Prices</h2>
            </div>
        </div>
        <div class="card border-light  mb-3 price-box text-center">
            <div class="card-header border-0 heading-card">One Time</div>
            <div class="card-body">
                <h5 class="card-title">18 €<span>/hr</span> </h5>
                <ul class="card-text">
                    <li>Lower prices</li>
                    <li>Easy online & secure payment</li>
                    <li>Easy amendment</li>
                </ul>
            </div>
        </div>
    </div>
</section>

<section class="Extra-services pt-0" id="Extra-services">
    <div class="max-width-1140">
        <div class="row justify-content-center text-center">
            <div class="col-12 mb-5 mb-75" style="position: relative;">
                <h2 class="headding-star">Extra services</h2>
            </div>
        </div>
        <div class="other-services">
            <div class="other-services-box text-center">
                <div class="icon-box">
                    <img src="~/img/prices/3.png" alt="forma-1" class="img-size">
                </div>
                <h6> Inside cabinets</h6>
                <h5>30 minutes</h5>
            </div>

            <div class="other-services-box text-center">
                <div class="icon-box">
                    <img src="~/img/prices/5.png" alt="group-22" class="img-size">
                </div>
                <h6>Inside fridge</h6>
                <h5>30 minutes</h5>
            </div>

            <div class="other-services-box text-center">
                <div class="icon-box">
                    <img src="~/img/prices/4.png" alt="group-22" class="img-size">
                </div>
                <h6>Inside oven</h6>
                <h5>30 minutes</h5>
            </div>

            <div class="other-services-box text-center">
                <div class="icon-box">
                    <img src="~/img/prices/2.png" alt="group-22" class="img-size">
                </div>
                <h6> Laundry wash & dry</h6>
                <h5>30 minutes</h5>
            </div>

            <div class="other-services-box text-center">
                <div class="icon-box">
                    <img src="~/img/prices/1.png" alt="group-22" class="img-size">
                </div>
                <h6> Interior windows</h6>
                <h5>30 minutes</h5>
            </div>
        </div>
    </div>
</section>


<section class="cleaning" id="cleaning">
    <div class="max-width">
        <div class="row justify-content-center text-center">
            <div class="col-12 mb-5 mb-75" style="position: relative;">
                <h2 class="headding-star">What we include in cleaning</h2>
            </div>
        </div>
        <div class="row our-blog cleaning-img-box">
            <div class="col-md-12">
                <div class="row row-cols-1 row-cols-sm-1 row-cols-md-2 row-cols-lg-3 g-4 justify-content-center">
                    <div class="col p-lr-22">
                        <div class="card h-100">
                            <img src="~/img/prices/group-18_3.png" class="card-img-top" alt="...">
                            <div class="card-body cleaning-text-box p-tb-10">
                                <h5 class="card-title mb-2">Bedroom and Living Room</h5>
                                <ul class="points">
                                    <li>
                                        Included Dust all accessible surfaces </li>
                                    <li>Included Wipe down all mirrors and glass fixtures</li>
                                    <li>Not Included Clean all floor surfaces</li>
                                    <li>Included Take out garbage and recycling</li>
                                    <li>Included To clean up</li>
                                    <li>Included Make beds</li>
                                </ul>

                            </div>
                        </div>
                    </div>
                    <div class="col p-lr-22">
                        <div class="card h-100">
                            <img src="~/img/prices/group-18_4.png" class="card-img-top" alt="...">
                            <div class="card-body cleaning-text-box p-tb-10">
                                <h5 class="card-title mb-2">Bathrooms</h5>
                                <ul class="points">
                                    <li>Included Wash and sanitize the toilet, shower, tub, sink</li>
                                    <li>Not Included Dust all accessible surfaces</li>
                                    <li>Included Wipe down all mirrors and glass fixtures</li>
                                    <li>Not Included Clean all floor surfaces</li>
                                    <li>Included Take out garbage and recycling</li>
                                </ul>


                            </div>
                        </div>
                    </div>
                    <div class="col p-lr-22">
                        <div class="card h-100">
                            <img src="~/img/prices/group-18_2.png" class="card-img-top" alt="...">
                            <div class="card-body cleaning-text-box p-tb-10">
                                <h5 class="card-title mb-2">Kitchen</h5>
                                <ul class="points">
                                    <li>Cleaning the sink</li>
                                    <li>Cleaning the oven (outside)</li>
                                    <li>Included Cleaning the work surfaces</li>
                                    <li>Included Dust all accessible surfaces</li>
                                    <li>Included Clean all floor surfaces</li>
                                    <li>Included Take out garbage and recycling</li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<section class="Why-helperland mt-30 m-5" id="Why-helperland">
    <div class="max-width p-0">
        <div class="row justify-content-center text-center">
            <div class="col-12 mb-5" style="position: relative;">
                <h2 class="headding-star">Why Why-helperland</h2>
            </div>
        </div>
        <div class="d-flex justify-content-center Why-helperland-box">
            <div class="card text-end" style="max-width: 377px;">
                <div class="card-body">
                    <div class="text-1">
                        <h5 class="card-title">Experienced and vetted professionals</h5>
                        <p class="card-text">
                            dominate the industry in scale and scope with an adaptable, extensive
                            network that consistently delivers exceptional results.
                        </p>
                    </div>
                    <div class="text-2">
                        <h5 class="card-title">Dedicated customer service</h5>
                        <p class="card-text">
                            to our customers and are guided in all we do by their needs. The team
                            is always happy to support you and offer all the information. you need.
                        </p>
                    </div>
                </div>
            </div>
            <div class="helperland-img d-flex align-items-center justify-content-center">
                <img src="~/img/prices/the-best-img-1.png" alt="">
            </div>
            <div class="card" style="max-width: 377px;">
                <div class="card-body">
                    <div class="text-1">
                        <h5 class="card-title">Every cleaning is insured</h5>
                        <p class="card-text">
                            and seek to provide exceptional service and engage in proactive
                            behavior. We‘d be happy to clean your homes.
                        </p>
                    </div>
                    <div class="text-2">
                        <h5 class="card-title">Secure online payment</h5>
                        <p class="card-text">
                            Payment is processed securely online. Customers pay safely online and
                            manage the booking.
                        </p>
                    </div>
                </div>
            </div>
        </div>

    </div>
</section>
