        function reflectrefund() {
            if ($("#txtAmountToRefund").val().length == 0) {
                $("#idcalculate").val("");
            }
            else {
                var refundamount = parseFloat($("#txtAmountToRefund").val());
                var refundtype = $("#ddlRefundType").val();
                var calculateamount;
                if (refundtype == "Percentage") {
                    calculateamount = (parseFloat($("#pamount").html()) * refundamount) / 100;
                }
                else {
                    // calculateamount = parseInt($("#pamount").html()) - refundamount;
                    calculateamount = parseFloat($("#txtAmountToRefund").val());

                }
                /*spnerrorrefund*/
                console.log(typeof (calculateamount));
                console.log(typeof (parseFloat($("#ibamount").html())));
                if (calculateamount > (parseFloat($("#ibamount").html()))) {
                    $("#spnerrorrefund").html("Refund Amount is greaterthan the paidamount");
                    $("#btnrefund").addClass("disable");
                }
                else {
                    $("#spnerrorrefund").html("");
                    $("#idcalculate").val(calculateamount);
                    $("#btnrefund").removeClass("disable");
                }
            }
        }
        function Refund() {
            var refundobj = {}

            refundobj.ServiceRequestID = $("#hiddenservicerequest").val();
            refundobj.refundamount = $("#idcalculate").val();
            refundobj.comments = $("#admincommetforrefund").val();
            $.ajax({
                type: 'post',
                url: '/Admin/refundservicepost',
                dataType: 'json',
                contentType: 'application/json',
                data: JSON.stringify(refundobj),
                success: function (respo) {
                    // $('#example').DataTable().ajax.reload();
                    $("#loader").removeClass("is-active");
                    console.log(respo);
                },
                error: function (err) {
                    $("#loader").removeClass("is-active");
                    console.log(err);
                }
            });
